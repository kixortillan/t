{
  "name": "mvp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bulma-slider": "^2.0.0",
    "d3": "^5.6.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.5"
  },
  "scripts": {
    "start": "react-scripts start && npm run watch-css",
    "build": "react-scripts build && npm run build-css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path ./node_modules/ --output-style compressed src/ -o src/",
    "watch-css": "node-sass-chokidar --include-path ./node_modules/ src/ -o src/ --watch --recursive"
  },
  "devDependencies": {
    "bulma": "^0.7.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.3"
  }
}
